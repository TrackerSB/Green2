<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bayern.steinbrecher.gruen2.launcher.LauncherController">
    <ProgressBar minWidth="200" progress="${controller.percentage}" />
    <HBox alignment="CENTER">
        <Label text="${controller.percentageString}" />
        <Label text="%downloaded" />
    </HBox>
</VBox>
